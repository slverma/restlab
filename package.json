{
  "name": "restlab",
  "displayName": "RESTLab",
  "description": "REST API testing and management tool",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "restlab-sidebar",
          "title": "RESTLab",
          "icon": "resources/restlab-icon.svg"
        }
      ]
    },
    "views": {
      "restlab-sidebar": [
        {
          "type": "webview",
          "id": "restlab-sidebar-view",
          "name": "RESTLab",
          "icon": "resources/restlab-icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "restlab.createFolder",
        "title": "Create Folder",
        "category": "RESTLab"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "compile": "npm run build",
    "build": "node esbuild.js",
    "watch": "node esbuild.js --watch"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/vscode": "^1.85.0",
    "@types/vscode-webview": "^1.57.4",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.10",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "form-data": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
